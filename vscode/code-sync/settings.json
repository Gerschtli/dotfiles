{
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.dragAndDrop": false,
    "editor.fontFamily": "'Ubuntu Mono'",
    "editor.formatOnSave": true,
    "editor.links": false,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.parameterHints.cycle": true,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        120
    ],
    "editor.showFoldingControls": "always",
    "editor.tabCompletion": "on",
    "editor.tokenColorCustomizations": {
        "[Material Theme High Contrast]": {
            "textMateRules": [
                {
                    "name": "Keyword Control",
                    "scope": [
                        "keyword.control"
                    ],
                    "settings": {
                        "fontStyle": "",
                        "foreground": "#C792EA"
                    }
                }
            ]
        }
    },
    "explorer.confirmDelete": false,
    "files.autoSave": "onFocusChange",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "materialTheme.accent": "Teal",
    "materialTheme.autoApplyIcons": true,
    "php.suggest.basic": false,
    "search.useIgnoreFiles": false,
    "sortLines.filterBlankLines": true,
    "update.channel": "none",
    "window.menuBarVisibility": "toggle",
    "window.zoomLevel": -1,
    "workbench.colorCustomizations": {
        "list.errorForeground": "#d4b437",
        "list.warningForeground": "#d4b437",
        "gitDecoration.modifiedResourceForeground": "#d4b437",
        "gitDecoration.untrackedResourceForeground": "#f47878"
    },
    "workbench.colorTheme": "Material Theme High Contrast",
    "workbench.iconTheme": "vscode-icons",
    "workbench.startupEditor": "newUntitledFile"
}