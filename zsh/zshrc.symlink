source "${HOME}/.dotfiles-variables"
source "${DOTFILES_ROOT}/util/script-functions.sh"

source_files -name '*.zsh' ! -name 'path.zsh' ! -name 'completion.zsh'

autoload -U compinit && compinit

source_files -name 'completion.zsh'

source "${DOTFILES_ROOT}/util/unset-script-functions.sh"
source "${DOTFILES_ROOT}/util/unset-variables.sh"
