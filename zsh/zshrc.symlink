source "${HOME}/.dotfiles-variables"
source "${DOTFILES_ROOT}/util/script-functions"

_d_source_files \( -name "*.sh" -o -name "*.zsh" \) \
    ! -name "path.sh" ! -name "path.zsh" \
    ! -name "completion.sh" ! -name "completion.zsh"

autoload -Uz +X compinit && compinit

_d_source_files \( -name "completion.sh" -o -name "completion.zsh" \)

source "${DOTFILES_ROOT}/util/unset-script-functions"
source "${DOTFILES_ROOT}/util/unset-variables"
